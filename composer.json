{
    "name": "moehakram/phpquick",
    "description": "Library PHP untuk framework webphp-oop",
    "type": "library",
    "require": {
        "php": "^8.1",
        "psr/container": "^2.0",
        "firebase/php-jwt": "^6.10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MA\\PHPQUICK\\": ""
        },
        "files": [
            "helpers/logging_helpers.php",
            "helpers/app_helpers.php",
            "helpers/string_helpers.php",
            "helpers/view_helpers.php",
            "helpers/base_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test" : "pest",
        "test-filter" : "pest --filter"
    },
    "authors": [
        {
            "name": "Muhammad Akram",
            "email": "moeh.akram@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/moehakram/webphp-oop-engine"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^2.35"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
